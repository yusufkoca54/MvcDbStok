@model MvcDbStok.Models.Entity.TblKategoriler
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form method="post" class="form-group">

   

    <div>
        <label>Kategori Adı</label>
        @Html.TextBoxFor(m=>m.KategoriAd,new {@class="form-control"})
        @Html.ValidationMessageFor(m=>m.KategoriAd,"",new {@style="color:blue"})
    </div>

    <div style="margin-top:15px;">
        <button class="btn btn-info">Kategori Ekle</button>
    </div>

</form>

